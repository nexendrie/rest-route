{
    "name": "nexendrie/rest-route",
    "description": "Rest route for Nette Framework",
    "keywords": [
        "nette",
        "routing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Štipák",
            "homepage": "https://newpope.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nexendrie\\RestRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\RestRoute\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "nette/application": "^3.0",
        "nette/http": "^3.0",
        "nette/utils": "^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/phpstan": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1.x-dev"
        }
    }
}
